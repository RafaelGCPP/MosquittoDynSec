{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "edge",
      "request": "launch",
      "name": "localhost (Edge)",
      "url": "https://localhost:4200",
      "webRoot": "${workspaceFolder}",
      "env": {
        "ASPNETCORE_HTTPS_PORT": 7044,
        "ASPNETCORE_HTTP_PORT": 5045
      }
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "localhost (Chrome)",
      "url": "https://localhost:4200",
      "webRoot": "${workspaceFolder}",
      "env": {
        "ASPNETCORE_HTTPS_PORT": 7044,
        "ASPNETCORE_HTTP_PORT": 5045
      }
    }
  ]
}
